{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "build",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceRoot}/src/*.c",
        "-o",
        "${workspaceRoot}/bin/a.out"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "调试器生成的任务。"
    }
  ],
  "version": "2.0.0"
}