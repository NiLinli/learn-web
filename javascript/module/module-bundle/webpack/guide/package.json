    "module-external": "webpack --config ./module/external/webpack.config.js",
    "plugin": "webpack --config ./plugin/custom-plugin/webpack.config.js",
    "output": "webpack --config ./output/webpack.config.js",
    "output-tree-shaking": "webpack --config ./output-optimization/tree-shaking/webpack.config.js",
    "output-code-splitting": "webpack --config ./output-optimization/code-splitting/webpack.config.js",
    "hot-module-replacement": "webpack-dev-server --open  --config ./hot-module-replacement/webpack.config.js",
    "development": "webpack serve --config ./development/webpack.config.js",
    "development-server": "webpack-dev-server --open --config ./development/webpack.config.js",
    "development-server-node": "node ./3-development/server.js",
