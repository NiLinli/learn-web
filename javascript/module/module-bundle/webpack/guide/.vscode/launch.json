  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8082",
      "webRoot": "${workspaceFolder}/development/src",
      "sourceMapPathOverrides": {
        // ${workspaceFolder} and ${webRoot}
        "webpack://guide/./development/src/*": "${webRoot}/*"
        // webpack-internal:///./development/src/print.js:8:5
      }
    }
  ]
}
